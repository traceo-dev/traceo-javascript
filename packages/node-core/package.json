{
    "name": "@traceo-sdk/node-core",
    "version": "0.33.0",
    "author": "Traceo",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/traceo-dev/traceo-sdk",
